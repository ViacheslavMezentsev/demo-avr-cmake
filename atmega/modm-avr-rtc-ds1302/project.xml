<library>
  <extends>modm:arduino-nano</extends>
  <options>
    <option name="modm:target">atmega328p-au</option>
    <option name="modm:platform:core:f_cpu">16M</option>
    <option name="modm:build:build.path">./build</option>
    <option name="modm:io:with_printf">yes</option>
    <option name="modm:build:cmake:include_cmakelists">false</option>
  </options>
  <modules>
    <module>modm:driver:ds1302</module>
    <module>modm:platform:clock</module>
    <module>modm:platform:core</module>
    <module>modm:platform:gpio</module>
    <module>modm:platform:uart:0</module>
    <module>modm:processing:timer</module>
    <!--<module>modm:build:scons</module>-->
    <module>modm:build:cmake</module>
  </modules>
</library>
